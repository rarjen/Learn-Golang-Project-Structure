            "get": {
                "description": "Melakukan ping ke database untuk memeriksa kesehatan aplikasi dan database",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "pingdb"
                ],
                "summary": "Ping the DB",
                "operationId": "get-pingdb",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/domain.PingDBResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        },
        "/sync-data/check-debitur-id": {
            "get": {
                "description": "Mendapatkan detail dari nasabah berdasarkan nasabahID",
